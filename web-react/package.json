{
  "name": "web-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "build": "NODE_ENV=\"production\" node ./build.js",
    "watch": "run-p watch:*",
    "watch:build": "sh -exec 'NODE_ENV=\\\"development\\\" node ./build.js'",
    "watch:serve": "wait-on public/index.js && reload -d public -b"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "esbuild": "^0.11.14",
    "npm-run-all": "^4.1.5",
    "reload": "^3.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "wait-on": "^5.3.0"
  }
}